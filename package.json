{
  "name": "@codebundlesbyvik/simple-notifier",
  "version": "0.9.0",
  "description": "A lightweight and easy to use notification library.",
  "main": "./src/js/index.js",
  "scripts": {
    "css-compile": "sass ./src/scss/simple-notifier.scss ./dist/simple-notifier.css --style=compressed",
    "css-prefix": "postcss -r ./dist/simple-notifier.css -m -u autoprefixer",
    "css-build": "npm-run-all css-compile css-prefix",
    "js-build": "webpack --mode production --config webpack.config.js",
    "build-all": "npm-run-all css-build js-build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vikputthiscodeongit/simple-notifier"
  },
  "keywords": [
    "simple",
    "easy",
    "light",
    "notifier",
    "notification",
    "notifications",
    "alert",
    "alerts",
    "information"
  ],
  "author": "codebundlesbyvik",
  "license": "MIT",
  "bugs": "https://github.com/vikputthiscodeongit/simple-notifier/issues",
  "dependencies": {
    "@codebundlesbyvik/css-media-functions": "^1.0.0-2",
    "@codebundlesbyvik/element-operations": "^1.0.0-2",
    "@codebundlesbyvik/number-operations": "^1.0.1-1",
    "css-duration": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "sass-rem": "^3.0.0",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2"
  },
  "browserslist": [
    "since 2019-01 and > 0.5%",
    "last 2 versions and not dead",
    "Firefox ESR",
    "not Explorer >= 0",
    "not OperaMini all"
  ]
}
